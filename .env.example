# Copy this file to .env and fill in your keys (OPTIONAL)
# Users can now provide API keys directly in the web UI when creating projects

# OpenAI API key (used by agents configured with model="openai")
# Only needed if you want to set default keys server-side
OPENAI_API_KEY=

# Google GenAI (Gemini) API key (used by agents configured with model="gemini")
# Only needed if you want to set default keys server-side
GEMINI_API_KEY=

# Perplexity API key (used by agents configured with model="perplexity")
# Only needed if you want to set default keys server-side
PERPLEXITY_API_KEY=

# PORT the server should listen on (default 5000)
PORT=5000

# Optional: NODE_ENV (development | production)
NODE_ENV=development
